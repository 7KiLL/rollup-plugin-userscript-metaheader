{
  "name": "rollup-plugin-userscript-metaheader",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A Rollup plugin to automatically generate userscript meta headers.",
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "userscript",
    "userscripts",
    "metaheader",
    "rollup",
    "vite",
    "tampermonkey"
  ],
  "author": {
    "email": "7killua@gmail.com",
    "name": "7KiLL"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "homepage": "https://github.com/7KiLL/rollup-plugin-userscript-metaheader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7KiLL/rollup-plugin-userscript-metaheader.git"
  },
  "bugs": {
    "url": "https://github.com/7KiLL/rollup-plugin-userscript-metaheader/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.3.2",
    "@vitest/coverage-v8": "^0.32.2",
    "husky": "^8.0.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "standard-version": "^9.5.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^3.0.2",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "rollup": "^1.20.0||^2.0.0||^3.0.0"
  },
  "main": "./dist/main.js",
  "module": "./dist/main.cjs",
  "typings": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist"
  ]
}
